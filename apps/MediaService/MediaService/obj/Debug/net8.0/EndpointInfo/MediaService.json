{
  "openapi": "3.0.1",
  "info": {
    "title": "MediaService",
    "version": "1.0"
  },
  "paths": {
    "/api/Media/image/{folder}/{fileName}": {
      "get": {
        "tags": [
          "Media"
        ],
        "parameters": [
          {
            "name": "folder",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "fileName",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Media/generate-qrs": {
      "post": {
        "tags": [
          "Media"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/QRModel"
                }
              }
            },
            "text/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/QRModel"
                }
              }
            },
            "application/*+json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/QRModel"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Media/assignment/pdf": {
      "post": {
        "tags": [
          "Media"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AssetAssignmentModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AssetAssignmentModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AssetAssignmentModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Media/assignment/zip": {
      "post": {
        "tags": [
          "Media"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/AssetAssignmentModel"
                }
              }
            },
            "text/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/AssetAssignmentModel"
                }
              }
            },
            "application/*+json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/AssetAssignmentModel"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Media/ImportAssets": {
      "post": {
        "tags": [
          "Media"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "file": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AssetAssignment": {
        "type": "object",
        "properties": {
          "detailID": {
            "type": "integer",
            "format": "int32"
          },
          "assetID": {
            "type": "integer",
            "format": "int32"
          },
          "assetTag": {
            "type": "string",
            "nullable": true
          },
          "templateName": {
            "type": "string",
            "nullable": true
          },
          "serialNumber": {
            "type": "string",
            "nullable": true
          },
          "unit": {
            "type": "string",
            "nullable": true
          },
          "quantity": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AssetAssignmentModel": {
        "type": "object",
        "properties": {
          "assignmentCode": {
            "type": "string",
            "nullable": true
          },
          "employeeName": {
            "type": "string",
            "nullable": true
          },
          "employeeCode": {
            "type": "string",
            "nullable": true
          },
          "departmentName": {
            "type": "string",
            "nullable": true
          },
          "assignerCode": {
            "type": "string",
            "nullable": true
          },
          "assignmentByName": {
            "type": "string",
            "nullable": true
          },
          "assignerDepartment": {
            "type": "string",
            "nullable": true
          },
          "assignmentAction": {
            "type": "string",
            "nullable": true
          },
          "assignmentDate": {
            "type": "string",
            "nullable": true
          },
          "notes": {
            "type": "string",
            "nullable": true
          },
          "assetAssignments": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AssetAssignment"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "QRModel": {
        "type": "object",
        "properties": {
          "assetTag": {
            "type": "string",
            "nullable": true
          },
          "assetName": {
            "type": "string",
            "nullable": true
          },
          "purchaseDate": {
            "type": "string",
            "nullable": true
          },
          "assetURL": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}